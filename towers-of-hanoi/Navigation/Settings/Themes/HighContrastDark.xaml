<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Background" Color="#000000"/>
    <SolidColorBrush x:Key="Foreground" Color="#000000"/>
    <SolidColorBrush x:Key="FocusedForeground" Color="#777777"/>
    <SolidColorBrush x:Key="DisabledForeground" Color="#000000"/>
    <SolidColorBrush x:Key="BorderColour" Color="#FFFF00" />
    <SolidColorBrush x:Key="DisabledBorderColour" Color="#FFFF00" />
    <SolidColorBrush x:Key="FocusedBorderColour" Color="#FFFF00" />
    <SolidColorBrush x:Key="AccentBorderColour" Color="#FFFF00" />
    <Thickness x:Key="BorderThickness">2</Thickness>
    <Thickness x:Key="BottomlessBorderThickness">2,2,2,0</Thickness>
    <Thickness x:Key="TabControlMargin">-2,-2,-2,-1</Thickness>
    <SolidColorBrush x:Key="StandardText" Color="#FFFFFF"/>
