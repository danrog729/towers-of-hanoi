<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <LinearGradientBrush x:Key="Background" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Color="#FF0000" Offset="0.0" />
        <GradientStop Color="#FF8800" Offset="0.125" />
        <GradientStop Color="#FFFF00" Offset="0.25" />
        <GradientStop Color="#88FF00" Offset="0.375" />
        <GradientStop Color="#00FF00" Offset="0.5" />
        <GradientStop Color="#00FF88" Offset="0.625" />
        <GradientStop Color="#00FFFF" Offset="0.75" />
        <GradientStop Color="#0088FF" Offset="0.875" />
        <GradientStop Color="#0000FF" Offset="1.0" />
    </LinearGradientBrush>
    
    <RadialGradientBrush x:Key="Foreground" GradientOrigin="0.5,0.5" Center="0.5,0.5">
        <GradientStop Color="#BBFFFF" Offset="0.0" />
        <GradientStop Color="#00BBBB" Offset="1.0" />
    </RadialGradientBrush>

    <RadialGradientBrush x:Key="FocusedForeground" GradientOrigin="0.5,0.5" Center="0.5,0.5">
        <GradientStop Color="#FFFFFF" Offset="0.0" />
        <GradientStop Color="#BBFFFF" Offset="1.0" />
    </RadialGradientBrush>

    <RadialGradientBrush x:Key="DisabledForeground" GradientOrigin="0.5,0.5" Center="0.5,0.5">
        <GradientStop Color="#77BBBB" Offset="0.0" />
        <GradientStop Color="#337777" Offset="1.0" />
    </RadialGradientBrush>

    <SolidColorBrush x:Key="BorderColour" Color="#00FF00" />
    <SolidColorBrush x:Key="DisabledBorderColour" Color="#00FF00" />
    <SolidColorBrush x:Key="FocusedBorderColour" Color="#00FF00" />
    <SolidColorBrush x:Key="AccentBorderColour" Color="#00FF00" />
    <Thickness x:Key="BorderThickness">2</Thickness>
    <Thickness x:Key="BottomlessBorderThickness">2,2,2,0</Thickness>
    <Thickness x:Key="TabControlMargin">-2,-2,-2,-1</Thickness>
    <SolidColorBrush x:Key="StandardText" Color="#222222"/>
    <SolidColorBrush x:Key="ImportantText" Color="Black"/>
    <SolidColorBrush x:Key="FocusedText" Color="#000000"/>
    <SolidColorBrush x:Key="DisabledText" Color="Transparent"/>
    
    <RadialGradientBrush x:Key="AccentColour" GradientOrigin="0.5,0.5" Center="0.5,0.5">
        <GradientStop Color="#FFFFFF" Offset="0.0" />
        <GradientStop Color="#FFFF00" Offset="1.0" />
    </RadialGradientBrush>
    
    <RadialGradientBrush x:Key="FocusedAccent" GradientOrigin="0.5,0.5" Center="0.5,0.5">
        <GradientStop Color="#FFFFFF" Offset="0.0" />
        <GradientStop Color="#AAFF00" Offset="1.0" />
    </RadialGradientBrush>
    
    <SolidColorBrush x:Key="AccentText" Color="Black"/>
    
    <LinearGradientBrush x:Key="FocusedAccentText" StartPoint="0,0" EndPoint="0,0">
        <GradientStop Color="#000000" Offset="0.0"/>
        <GradientStop Color="#FFFFFF" Offset="1.0"/>
        <GradientStop Color="#000000" Offset="0.0"/>
    </LinearGradientBrush>

    <FontFamily x:Key="Font">Comic Sans MS</FontFamily>

    <SolidColorBrush x:Key="Disc0" Color="Red"/>
    <SolidColorBrush x:Key="Disc1" Color="Orange"/>
    <SolidColorBrush x:Key="Disc2" Color="Yellow"/>
    <SolidColorBrush x:Key="Disc3" Color="Green"/>
    <SolidColorBrush x:Key="Disc4" Color="Blue"/>
    <SolidColorBrush x:Key="Disc5" Color="Purple"/>
    <SolidColorBrush x:Key="Disc6" Color="Red"/>
    <SolidColorBrush x:Key="Disc7" Color="Orange"/>
    <SolidColorBrush x:Key="Disc8" Color="Yellow"/>
    <SolidColorBrush x:Key="Disc9" Color="Green"/>
    <SolidColorBrush x:Key="Disc10" Color="Blue"/>
    <SolidColorBrush x:Key="Disc11" Color="Purple"/>
    <SolidColorBrush x:Key="Disc12" Color="Red"/>
    <SolidColorBrush x:Key="Disc13" Color="Orange"/>
    <SolidColorBrush x:Key="Disc14" Color="Yellow"/>
    <SolidColorBrush x:Key="Disc15" Color="Green"/>
    <SolidColorBrush x:Key="Disc16" Color="Blue"/>
    <SolidColorBrush x:Key="Disc17" Color="Purple"/>
    <SolidColorBrush x:Key="Disc18" Color="Red"/>
    <SolidColorBrush x:Key="Disc19" Color="Orange"/>

    <SolidColorBrush x:Key="PoleColour" Color="Blue"/>

</ResourceDictionary>