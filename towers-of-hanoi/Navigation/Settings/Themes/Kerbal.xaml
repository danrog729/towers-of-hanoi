<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <LinearGradientBrush x:Key="Background" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#5E616A" Offset="0.0"/>
        <GradientStop Color="#343840" Offset="1.0"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="Foreground" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#5E616A" Offset="0.0"/>
        <GradientStop Color="#5E616A" Offset="0.45"/>
        <GradientStop Color="#4C4F57" Offset="0.50"/>
        <GradientStop Color="#33373F" Offset="1.0"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="FocusedForeground" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#B9BEC0" Offset="0.0"/>
        <GradientStop Color="#899396" Offset="0.45"/>
        <GradientStop Color="#757E82" Offset="0.50"/>
        <GradientStop Color="#9CA1A3" Offset="1.0"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="DisabledForeground" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#50535C" Offset="0.0"/>
        <GradientStop Color="#4F525A" Offset="0.45"/>
        <GradientStop Color="#454851" Offset="0.50"/>
        <GradientStop Color="#373B43" Offset="1.0"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="BorderColour" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#85888F" Offset="0.0"/>
        <GradientStop Color="#17171A" Offset="1.0"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="DisabledBorderColour" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#64676F" Offset="0.0"/>
        <GradientStop Color="#282A30" Offset="1.0"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="FocusedBorderColour" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#BBBEC0" Offset="0.0"/>
        <GradientStop Color="#818688" Offset="1.0"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="AccentBorderColour" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#BCE0E6" Offset="0.0"/>
        <GradientStop Color="#17919F" Offset="0.5"/>
        <GradientStop Color="#00393E" Offset="1.0"/>
    </LinearGradientBrush>

    <Thickness x:Key="BorderThickness">2</Thickness>
    <Thickness x:Key="BottomlessBorderThickness">2,2,2,0</Thickness>
    <Thickness x:Key="TabControlMargin">-2,-2,-2,-1</Thickness>
    <SolidColorBrush x:Key="StandardText" Color="#D4F4FD"/>
    <SolidColorBrush x:Key="ImportantText" Color="#B7FE00"/>
    <SolidColorBrush x:Key="FocusedText" Color="#D4F4FD"/>
    <SolidColorBrush x:Key="DisabledText" Color="#8FA1AA"/>
    
    <LinearGradientBrush x:Key="AccentColour" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#1FC0CF" Offset="0.0"/>
        <GradientStop Color="#18BECE" Offset="0.45"/>
        <GradientStop Color="#01A6B6" Offset="0.50"/>
        <GradientStop Color="#00717B" Offset="1.0"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="FocusedAccent" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#61F7FF" Offset="0.0"/>
        <GradientStop Color="#42F7FF" Offset="0.45"/>
        <GradientStop Color="#00D3EA" Offset="0.50"/>
        <GradientStop Color="#00A6B5" Offset="1.0"/>
    </LinearGradientBrush>
    
    <SolidColorBrush x:Key="AccentText" Color="#16424D"/>
    <SolidColorBrush x:Key="FocusedAccentText" Color="#EFFFDF"/>
    <FontFamily x:Key="Font">Corbel Bold</FontFamily>

    <SolidColorBrush x:Key="Disc0" Color="#333333"/>
    <SolidColorBrush x:Key="Disc1" Color="#ffffff"/>
    <SolidColorBrush x:Key="Disc2" Color="#ffffff"/>
    <SolidColorBrush x:Key="Disc3" Color="#ffffff"/>
    <SolidColorBrush x:Key="Disc4" Color="#ffffff"/>
    <SolidColorBrush x:Key="Disc5" Color="#000000"/>
    <SolidColorBrush x:Key="Disc6" Color="#ffffff"/>
    <SolidColorBrush x:Key="Disc7" Color="#b31942"/>
    <SolidColorBrush x:Key="Disc8" Color="#0a3161"/>
    <SolidColorBrush x:Key="Disc9" Color="#000000"/>
    <SolidColorBrush x:Key="Disc10" Color="#ffffff"/>
    <SolidColorBrush x:Key="Disc11" Color="#ffffff"/>
    <SolidColorBrush x:Key="Disc12" Color="#000000"/>
    <SolidColorBrush x:Key="Disc13" Color="#ffffff"/>
    <SolidColorBrush x:Key="Disc14" Color="#b31942"/>
    <SolidColorBrush x:Key="Disc15" Color="#b31942"/>
    <SolidColorBrush x:Key="Disc16" Color="#ffffff"/>
    <SolidColorBrush x:Key="Disc17" Color="#000000"/>
    <SolidColorBrush x:Key="Disc18" Color="#ffffff"/>
    <SolidColorBrush x:Key="Disc19" Color="#aaaaaa"/>

    <SolidColorBrush x:Key="PoleColour" Color="Orange"/>

</ResourceDictionary>