<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Background" Color="#FFFF99"/>
    <SolidColorBrush x:Key="Foreground" Color="#FFFF99"/>
    <SolidColorBrush x:Key="FocusedForeground" Color="#FFFF99"/>
    <SolidColorBrush x:Key="DisabledForeground" Color="#FFFF99"/>
    <SolidColorBrush x:Key="BorderColour" Color="Transparent" />
    <SolidColorBrush x:Key="DisabledBorderColour" Color="Transparent" />
    <SolidColorBrush x:Key="FocusedBorderColour" Color="Transparent" />
    <SolidColorBrush x:Key="AccentBorderColour" Color="Transparent" />
    <Thickness x:Key="BorderThickness">0</Thickness>
    <Thickness x:Key="BottomlessBorderThickness">0</Thickness>
    <Thickness x:Key="TabControlMargin">-2,-2,-2,0</Thickness>
    <SolidColorBrush x:Key="StandardText" Color="#000000"/>
    <SolidColorBrush x:Key="ImportantText" Color="#CC0000"/>
    <SolidColorBrush x:Key="FocusedText" Color="#CC0000"/>
    <SolidColorBrush x:Key="DisabledText" Color="Transparent"/>
    <SolidColorBrush x:Key="AccentColour" Color="Transparent"/>
    <SolidColorBrush x:Key="FocusedAccent" Color="Transparent"/>
    <SolidColorBrush x:Key="AccentText" Color="#CC0000"/>
    <SolidColorBrush x:Key="FocusedAccentText" Color="#000000"/>
    <FontFamily x:Key="Font">Comic Sans MS</FontFamily>

</ResourceDictionary>