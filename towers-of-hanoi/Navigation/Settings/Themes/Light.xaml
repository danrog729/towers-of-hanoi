<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:System="clr-namespace:System;assembly=mscorlib">

    <SolidColorBrush x:Key="Background" Color="#aaaaaa"/>
    <SolidColorBrush x:Key="Foreground" Color="#cccccc"/>
    <SolidColorBrush x:Key="FocusedForeground" Color="#ffffff"/>
    <SolidColorBrush x:Key="DisabledForeground" Color="#aaaaaa"/>
    <SolidColorBrush x:Key="BorderColour" Color="#888888" />
    <SolidColorBrush x:Key="DisabledBorderColour" Color="#888888" />
    <SolidColorBrush x:Key="FocusedBorderColour" Color="#888888" />
    <SolidColorBrush x:Key="AccentBorderColour" Color="#888888" />
    <Thickness x:Key="BorderThickness">1</Thickness>
    <Thickness x:Key="BottomlessBorderThickness">1,1,1,0</Thickness>
    <Thickness x:Key="TabControlMargin">-2,-2,-2,0</Thickness>
    <SolidColorBrush x:Key="StandardText" Color="#222222"/>
    <SolidColorBrush x:Key="ImportantText" Color="Black"/>
    <SolidColorBrush x:Key="FocusedText" Color="#000000"/>
    <SolidColorBrush x:Key="DisabledText" Color="#999999"/>
    <SolidColorBrush x:Key="AccentColour" Color="#51b4ff"/>
    <SolidColorBrush x:Key="FocusedAccent" Color="#84e7ff"/>
    <SolidColorBrush x:Key="AccentText" Color="Black"/>
    <SolidColorBrush x:Key="FocusedAccentText" Color="#000000"/>
    <FontFamily x:Key="Font">Segoe UI</FontFamily>

    <SolidColorBrush x:Key="Disc0" Color="#ff0000"/>
    <SolidColorBrush x:Key="Disc1" Color="#ee1100"/>
    <SolidColorBrush x:Key="Disc2" Color="#dd2200"/>
    <SolidColorBrush x:Key="Disc3" Color="#cc3300"/>
    <SolidColorBrush x:Key="Disc4" Color="#bb4400"/>
    <SolidColorBrush x:Key="Disc5" Color="#aa5500"/>
    <SolidColorBrush x:Key="Disc6" Color="#996600"/>
    <SolidColorBrush x:Key="Disc7" Color="#887700"/>
    <SolidColorBrush x:Key="Disc8" Color="#778800"/>
    <SolidColorBrush x:Key="Disc9" Color="#669900"/>
    <SolidColorBrush x:Key="Disc10" Color="#55aa00"/>
    <SolidColorBrush x:Key="Disc11" Color="#44bb00"/>
    <SolidColorBrush x:Key="Disc12" Color="#33cc00"/>
    <SolidColorBrush x:Key="Disc13" Color="#22dd00"/>
    <SolidColorBrush x:Key="Disc14" Color="#11ee00"/>
    <SolidColorBrush x:Key="Disc15" Color="#00ff00"/>
    <SolidColorBrush x:Key="Disc16" Color="#00ee11"/>
    <SolidColorBrush x:Key="Disc17" Color="#00dd22"/>
    <SolidColorBrush x:Key="Disc18" Color="#00cc33"/>
    <SolidColorBrush x:Key="Disc19" Color="#00bb44"/>

    <SolidColorBrush x:Key="PoleColour" Color="#222222"/>

</ResourceDictionary>